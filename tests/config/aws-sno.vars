# OKD 4.12 - AWS SingleReplica Topology

release_image: $RELEASE_IMAGE
release_version: $RELEASE_VERSION

cluster_name: sno-$DISTRIBUTION
config_base_domain: fake.okd.io

provider: aws
config_provider: aws
config_platform: none
cluster_profile: SingleReplica
config_cluster_region: us-east-1

config_ssh_key: "$CONFIG_SSH_KEY"
config_pull_secret_file: "{{ playbook_dir }}/../tests/config/pull-secret-okd-fake.json"
